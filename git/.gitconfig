[user]
    name  = natsume6075
    email = natsume6075@gmail.com
[alias]
    co = checkout
    tree = log --graph --all --format=\"%x09%C(cyan bold)%an%Creset%x09%C(yellow)%h%Creset %C(magenta reverse)%d%Creset %s\"

[core]
    editor = nvim

[diff]
    tool = vimdiff
[difftool]
    prompt = false
[difftool "vimdiff"]
    path = nvim

[merge]
    tool = vimdiff
[mergetool]
    prompt = false
[mergetool "vimdiff"]
    path = nvim

